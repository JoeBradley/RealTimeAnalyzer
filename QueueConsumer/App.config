<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <add key="RabbitMQHost" value="192.168.99.100"/>
    <!--<add key="RabbitMQHost" value="localhost"/>-->
    <add key="RabbitMQPort" value="5672"/>

    <!--Reids config-->
